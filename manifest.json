{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/images/get_started16.png",
        "32": "images/images/get_started32.png",
        "48": "images/images/get_started48.png",
        "128": "images/images/get_started128.png"
      }
    },
    "manifest_version": 2
  }